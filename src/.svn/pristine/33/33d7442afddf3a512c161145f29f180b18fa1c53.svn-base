<view class="we-container bg_fff {{directionClass}}">
    <view class="we-wrapper " id="mySwiper"
    	bindtouchstart="$bind"
	    bindtouchmove="$bind"
	    bindtouchend="$bind"
	    animation="{{animationData}}">
        <view class="we-slide">
			<image class="wh100"  mode="aspectFit" src="../../public/images/banner/banner.jpg" alt=""/>
		</view>
        <view class="we-slide">slide 2</view>
        <view class="we-slide">slide 3</view>
    </view>
</view>
<view class="weui-flex icon_list bg_fff">
	<view class="weui-flex__item pull-center">
		<image class="bg_00b7ee radius_m"  mode="aspectFit" src="../../public/images/home/icon1.png" alt=""/>
		<span>矿用设备</span>
	</view>
	<view class="weui-flex__item pull-center">
		<image class="bg_ae5da1 radius_m"  mode="aspectFit" src="../../public/images/home/icon2.png" alt=""/>
		<span>耗材药剂</span>
	</view>
	<view class="weui-flex__item pull-center">
		<image class="bg_f19149 radius_m"  mode="aspectFit" src="../../public/images/home/icon3.png" alt=""/>
		<span>金属矿产</span>
	</view>
	<view class="weui-flex__item pull-center">
		<image class="bg_32b16c radius_m"  mode="aspectFit" src="../../public/images/home/icon4.png" alt=""/>
		<span>能源矿产</span>
	</view>
</view>
