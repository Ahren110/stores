.borderright { border-right: 1px #ccc solid; }
.borderleft { border-left: 1px #ccc solid; }
.mc_flex { background: #fff; }
.mc_flex .flex_item { border-left: 1px #ccc solid; text-align: center; font-size: 13px; }
.mc_flex .flex_item:last-child { border-right: 1px #ccc solid; }
.weui-btn[type=blue] { background: #00a5f7; color: #fff; }
.weui-btn[type=blue]:active { background: #0087ca; }



